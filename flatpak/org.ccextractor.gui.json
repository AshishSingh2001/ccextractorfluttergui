{
    "app-id": "org.ccextractor.Gui",
    "runtime": "org.gnome.Platform",
    "runtime-version": "40",
    "sdk": "org.gnome.Sdk",
    "command": "ccxgui",
    "separate-locales": false,
    "finish-args": [
        "--share=ipc",
        "--socket=x11",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--share=network",
        "--filesystem=home",
        "--filesystem=xdg-documents"
    ],
    "modules": [
        {
            "name": "ccxgui",
            "buildsystem": "simple",
            "build-commands": [
                "cd ccxgui && mkdir lib/ data/ && mv flutter_assets icudtl.dat data/ && mv lib*.so lib/",
                "ls ccxgui",
                "cp -r ccxgui /app/ccxgui",
                "chmod +x /app/ccxgui/ccxgui",
                "mkdir /app/bin",
                "ln -s /app/ccxgui/ccxgui /app/bin/ccxgui"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://nightly.link/CCExtractor/ccextractorfluttergui/workflows/create_artifacts/master/CCExtractor%20Flutter%20GUI%20Linux.zip",
                    "dest": "ccxgui",
                    "sha256": "0cf6b3174fe3d08fda0d3414c6f9c96e2f73b96c777c4378a8c166bed599812c"
                }
            
              
            ]
        }
    ]
}
